/var/log/tengine/*.log {
        daily
        missingok
        rotate 52
        compress
        delaycompress
        notifempty
        create 644 tengine adm
        sharedscripts
        postrotate
                [ -f /var/run/tengine.pid ] && kill -USR1 `cat /var/run/tengine.pid`
        endscript
}
